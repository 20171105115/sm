<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.4//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.imooc.sm.dao.SelfDao">
    <select id="selectByAcount" parameterType="String" resultMap="com.imooc.sm.dao.StaffDao.resultMap">
        select * from staff where account=#{account};
    </select>
    <update id="changePassword">
        update staff set password=#{password} where id=#{id}
    </update>
</mapper>